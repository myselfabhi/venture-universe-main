{
  "extends": "next/core-web-vitals",
  "rules": {
    "react/prop-types": "off",
    "no-undef": "off",
    "react/no-unknown-property": [
      "error",
      {
        "ignore": [
          "dispose",
          "rotation",
          "position",
          "scale",
          "object",
          "geometry",
          "material",
          "skeleton"
        ]
      }
    ],
    "no-unused-vars": [
      "warn",
      {
        "varsIgnorePattern": "^React$"
      }
    ],
    "react/no-unescaped-entities": "off"
  }
}
